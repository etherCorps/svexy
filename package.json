{
	"name": "svexy-ui",
	"version": "0.1.0",
	"description": "A design system based Svelte Component Library",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"package": "svelte-kit package",
		"preview": "vite preview",
		"prepare": "svelte-kit sync",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint .",
		"format": "prettier --write --plugin-search-dir=. ."
	},
	"exports": {
		".": "./index.js"
	},
	"workspaces": [
		"src",
		"svexy-ui"
	],
	"devDependencies": {
		"@sveltejs/adapter-auto": "next",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"autoprefixer": "^10.4.7",
		"eslint": "^8.16.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"postcss": "^8.4.14",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"svelte": "^3.44.0",
		"svelte-check": "^2.7.1",
		"svelte-preprocess": "^4.10.7",
		"svelte2tsx": "^0.5.11",
		"tslib": "^2.3.1",
		"typescript": "^4.7.2",
		"vite": "^3.0.0"
	},
	"dependencies": {
		"classnames": "^2.3.1",
		"svelte-heros": "^2.2.2",
		"tailwindcss": "^3.1.4",
		"@sveltejs/kit": "^1.0.0-next.377"
	},
	"license": "MIT",
	"type": "module",
	"module": "index.js",
	"bugs": "https://github.com/etherCorps/svexy/issues",
	"keywords": [
		"svelte",
		"sveltekit",
		"tailwindcss",
		"svexy-ui",
		"svexy",
		"ui",
		"accordion",
		"alert",
		"avatar",
		"badges",
		"breadcrumbs",
		"button-groups",
		"buttons",
		"cards",
		"carousel",
		"darkmode",
		"dropdown",
		"footer",
		"forms",
		"list group",
		"modals",
		"navbar",
		"paginations",
		"progress bars",
		"sidebars",
		"spinners",
		"tables",
		"tabs",
		"timelines",
		"toasts",
		"tooltip"
	]
}
